pipeline {
        agent any

        stages {
                stage('Build') {
                        steps {
                                echo 'Hello I am Building..'
                        }
                }
                stage('Test') {
                        steps {
                                echo 'Now we are testing Testing..'
                        }
                }
                stage('Deploy') {
                        steps {
                                echo 'Deploying....'
                        }
                }
        }
}


